{
  "expo": {
    "name": "MindSpace",
    "slug": "MindSpace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/adaptive-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/icon-3.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "newArchEnabled": true
    },
    "notification": {
      "icon": "./app/assets/icon-3.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to share posts.",
        "UIBackgroundModes": ["remote-notification"]
      },
      "bundleIdentifier": "com.ssajid.MindSpace"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ],
      "package": "com.ssajid.MindSpace",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f2a24a1b-0deb-4b83-a975-c6d72901e8bd"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f2a24a1b-0deb-4b83-a975-c6d72901e8bd"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
